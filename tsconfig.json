{
  "compilerOptions": {
    "jsx": "react",
    "noEmitOnError": true,
    "inlineSources": true,
    "sourceMap": true,
    "lib": [
      "es6",
      "dom",
      "esnext.asynciterable"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "outDir": "build",
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types/"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "./"
  ]
}
